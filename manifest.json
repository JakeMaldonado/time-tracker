{
    "name": "Time Tacker ",
    "manifest_version": 2,
    "version": "1.0",
    "description": "Tracks time an activity takes and deducts it from expected lifespan",
    "permissions": ["storage"],
    "background": {
        "scripts": ["script/index.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html"
    }
}